time IsIsomorphic(SmallFusionSystem(5^6,1), SmallFusionSystem(5^6,2));
time IsIsomorphicFusionRecords(SmallFusionSystemRecord(5^6,1), SmallFusionSystemRecord(5^6,2));
F_2 := SmallFusionSystem(5^6,2);
time IsIsomorphicFusionRecords(SmallFusionSystemRecord(5^6,1), SmallFusionSystemRecord(5^6,2) : preloaded := F_2);

