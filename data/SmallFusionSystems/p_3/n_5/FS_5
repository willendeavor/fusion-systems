[ <E.1, E.2 * E.3^2 * E.4>, <E.2, E.1^2 * E.4>, <E.3, E.4>, <E.4, E.3^2> ],
[ <E.1, E.1 * E.2>, <E.2, E.2 * E.3^2 * E.4^2>, <E.3, E.3 * E.4>, <E.4, E.4> ],
[ <E.1, E.1^2 * E.3^2>, <E.2, E.2^2 * E.4^2>, <E.3, E.3^2>, <E.4, E.4^2> ],
[ <E.1, E.1^2 * E.3^2>, <E.2, E.2 * E.3>, <E.3, E.3^2>, <E.4, E.4> ],
[ <E.1, E.1^2 * E.2 * E.3^2>, <E.2, E.1 * E.2 * E.3^2>, <E.3, E.3^2 * E.4>, <E.4, E.3 * E.4> ]
], 
