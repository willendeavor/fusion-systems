intrinsic FusionRecordTemp() -> Rec 
 {} 

    FusionRecord := recformat<
		p : RngIntElt,
		S : Grp, 
		S_order: RngIntElt,
		S_name : MonStgElt,
		S_small_group_id : Tup, 
		EssentialData : SeqEnum,
		Core: Grp, 
		OpTriv : BoolElt,
		pPerfect: BoolElt,
		FocalSubgroup : Grp
		>;

	EssentialRecord := recformat< 
		E : Grp, 
		E_order : RngIntElt,
		E_name : MonStgElt,
		AutFE_order : RngIntElt,
		AutFE_name : MonStgElt,
		AutFE_gens : SeqEnum
		>; 

  S :=PCGroup(\[ 5, -3, 3, -3, -3, -3, 121, 636, 947, 147, 2163 ])
; 
EssentialData := [];

E := sub<S| { S.1, S.5, S.2, S.3, S.4 }>; 
ER := rec< EssentialRecord |
E := sub<S| { S.1, S.5, S.2, S.3, S.4 }>, 
E_order := 243, 
E_name := "He3:C3:C3", 
AutFE_order := 324, 
AutFE_gens := [
[ <E.1, E.1 * E.4^2 * E.5^2>, <E.2, E.2 * E.3>, <E.3, E.3 * E.4^2>, <E.4, E.4 * E.5>, <E.5, E.5> ],
[ <E.1, E.1^2>, <E.2, E.2^2 * E.4^2 * E.5>, <E.3, E.3 * E.4 * E.5^2>, <E.4, E.4^2 * E.5^2>, <E.5, E.5> ],
[ <E.1, E.1^2>, <E.2, E.2>, <E.3, E.3^2 * E.4^2>, <E.4, E.4 * E.5>, <E.5, E.5^2> ],
[ <E.1, E.1 * E.3^2 * E.5^2>, <E.2, E.2>, <E.3, E.3>, <E.4, E.4>, <E.5, E.5> ],
[ <E.1, E.1 * E.5>, <E.2, E.2>, <E.3, E.3>, <E.4, E.4>, <E.5, E.5> ],
[ <E.1, E.1>, <E.2, E.2>, <E.3, E.3>, <E.4, E.4>, <E.5, E.5> ],
[ <E.1, E.1 * E.5^2>, <E.2, E.2 * E.3>, <E.3, E.3 * E.4^2>, <E.4, E.4 * E.5>, <E.5, E.5> ]
], 
AutFE_name := "C3:D9:S3" 
	>; 
Append(~EssentialData, ER); 

E := sub<S| { S.3 * S.5^2, S.2 * S.4^2 * S.5, S.5, S.4 * S.5^2 }>; 
ER := rec< EssentialRecord |
E := sub<S| { S.3 * S.5^2, S.2 * S.4^2 * S.5, S.5, S.4 * S.5^2 }>, 
E_order := 81, 
E_name := "C9^2", 
