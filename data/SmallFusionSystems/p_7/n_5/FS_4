E := sub<S| [ S.1, S.3, S.4 * S.5, S.5 ]>, 
E_name := "C7^4", 
AutFE_gens := [
[ <S.1, S.1^3 * S.3^4 * S.4^5>, <S.3, S.1 * S.3^4 * S.4^5 * S.5>, <S.4 * S.5, S.1^3 * S.3^5 * S.4^5 * S.5>, <S.5, S.1^3 * S.3^6 * S.4^6> ],
[ <S.1, S.1^6>, <S.3, S.3^6>, <S.4 * S.5, S.4^6 * S.5^6>, <S.5, S.5^6> ],
[ <S.1, S.3 * S.4>, <S.3, S.3^2 * S.4^4 * S.5^3>, <S.4 * S.5, S.1^6 * S.4>, <S.5, S.1^3 * S.3^6 * S.4^4 * S.5> ],
