intrinsic FusionRecordTemp() -> Rec 
 {} 

    FusionRecord := recformat<
		p : RngIntElt,
		S : Grp, 
		S_order: RngIntElt,
		S_name : MonStgElt,
		S_small_group_id : Tup, 
		EssentialData : SeqEnum,
		Core: Grp, 
		OpTriv : BoolElt,
		pPerfect: BoolElt,
		FocalSubgroup : Grp
		>;

	EssentialRecord := recformat< 
		E : Grp, 
		E_order : RngIntElt,
		E_name : MonStgElt,
		AutFE_order : RngIntElt,
		AutFE_name : MonStgElt,
		AutFE_gens : SeqEnum
		>; 

  S :=PCGroup(\[ 5, -5, 5, -5, -5, -5, 301, 1207, 5508 ]: ExponentLimit := 1)
; 
EssentialData := [];

E := sub<S| { S.1, S.3, S.4, S.2, S.5 }>; 
ER := rec< EssentialRecord |
E := sub<S| { S.1, S.3, S.4, S.2, S.5 }>, 
E_order := 3125, 
E_name := "C5^2.He5", 
AutFE_order := 2500, 
AutFE_gens := [
[ <E.1, E.1^2>, <E.2, E.2^4>, <E.3, E.3^3 * E.4 * E.5^3>, <E.4, E.4^2 * E.5^2>, <E.5, E.5^3> ],
[ <E.1, E.1 * E.3^4>, <E.2, E.2>, <E.3, E.3 * E.4^3>, <E.4, E.4 * E.5^2>, <E.5, E.5> ],
[ <E.1, E.1>, <E.2, E.2 * E.5^3>, <E.3, E.3>, <E.4, E.4>, <E.5, E.5> ],
[ <E.1, E.1^4>, <E.2, E.2>, <E.3, E.3^4>, <E.4, E.4^4>, <E.5, E.5^4> ],
[ <E.1, E.1>, <E.2, E.2>, <E.3, E.3>, <E.4, E.4>, <E.5, E.5> ],
[ <E.1, E.1>, <E.2, E.2 * E.3>, <E.3, E.3>, <E.4, E.4>, <E.5, E.5> ],
[ <E.1, E.1>, <E.2, E.2 * E.4^2>, <E.3, E.3>, <E.4, E.4>, <E.5, E.5> ]
], 
AutFE_name := "(He5.C5):C4" 
	>; 
Append(~EssentialData, ER); 

E := sub<S| { S.1, S.3, S.4 * S.5^3, S.5 }>; 
ER := rec< EssentialRecord |
E := sub<S| { S.1, S.3, S.4 * S.5^3, S.5 }>, 
E_order := 625, 
E_name := "C5^4", 
AutFE_order := 320, 
AutFE_gens := [
[ <E.1, E.2^4 * E.3^4 * E.4^4>, <E.2, E.1^3 * E.3 * E.4>, <E.3, E.1 * E.3^4 * E.4>, <E.4, E.1 * E.2^4> ],
[ <E.1, E.2 * E.3^4 * E.4^4>, <E.2, E.1^2 * E.2^3 * E.3^4 * E.4^2>, <E.3, E.1^4 * E.2 * E.3 * E.4>, <E.4, E.1^4 * E.2^2 * E.3^3 * E.4> ],
[ <E.1, E.2^4 * E.3^2 * E.4>, <E.2, E.3^4 * E.4^4>, <E.3, E.1^4 * E.2^4 * E.3^4 * E.4^4>, <E.4, E.1 * E.2^2 * E.3 * E.4> ],
[ <E.1, E.1^4>, <E.2, E.2^4>, <E.3, E.3^4>, <E.4, E.4^4> ],
[ <E.1, E.1 * E.2^4 * E.3^3 * E.4>, <E.2, E.1 * E.2^2 * E.3^3>, <E.3, E.1^4 * E.2^2 * E.3^2 * E.4^3>, <E.4, E.1^2 * E.2^2 * E.3^4> ],
[ <E.1, E.4>, <E.2, E.1^3 * E.2 * E.3^2 * E.4^3>, <E.3, E.2^4 * E.3^4 * E.4^2>, <E.4, E.1^4> ],
[ <E.1, E.1^3 * E.2^4 * E.4^3>, <E.2, E.1^3 * E.3 * E.4^4>, <E.3, E.1^2 * E.2^3 * E.3^2 * E.4>, <E.4, E.1 * E.2 * E.3^2> ]
], 
AutFE_name := "C4.C2^3.D5" 
	>; 
Append(~EssentialData, ER); 
R := rec< FusionRecord |
p := 5,
S := S, 
S_order := 3125,
S_name := "C5^2.He5",
S_small_group_id := <3125, 30>,
EssentialData := EssentialData>;
return R; 
end intrinsic;