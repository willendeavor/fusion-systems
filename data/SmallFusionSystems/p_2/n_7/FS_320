[ <E.1, E.1>, <E.2, E.2 * E.4>, <E.3, E.3 * E.5>, <E.4, E.4>, <E.5, E.5 * E.7>, <E.6, E.6>, <E.7, E.7> ],
[ <E.1, E.1 * E.4>, <E.2, E.2>, <E.3, E.3 * E.6>, <E.4, E.4>, <E.5, E.5>, <E.6, E.6 * E.7>, <E.7, E.7> ],
[ <E.1, E.1 * E.5 * E.7>, <E.2, E.2 * E.6 * E.7>, <E.3, E.3>, <E.4, E.4>, <E.5, E.5 * E.7>, <E.6, E.6 * E.7>, <E.7, E.7> ]
], 
