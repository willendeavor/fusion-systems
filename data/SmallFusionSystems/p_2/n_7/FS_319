[ <E.1, E.1 * E.5>, <E.2, E.2>, <E.3, E.3 * E.4>, <E.4, E.4>, <E.5, E.5>, <E.6, E.6 * E.7>, <E.7, E.7> ]
], 
AutFE_name := "C2^2wrC2:C3" 
	>; 
